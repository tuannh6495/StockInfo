<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Tài chính Việt Nam </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <link rel="stylesheet" href="~/Views/Style/Layout.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div class="header">
            <div class="header_logo">
                    <div class="logo">
                        <h1>
                            <img src="https://cafefcdn.com/web_images/cafef_logo2022.svg">
                        </h1>
                    </div>       

                    <div class="maCK" id="CafeF_StockSymbolContainer">
                        <div id="macktheodoi" class="inner" data-type="stock-container">
                            <button class="maCK-button" title="Click vào đây để chọn mã chứng khoán cần theo dõi">
                                Chọn mã CK
                                <br>
                                cần theo dõi
                            </button>
                            <button class="maCK-button" title="Click vào đây để chọn mã chứng khoán cần theo dõi">
                                Chọn mã CK
                                <br>
                                cần theo dõi
                            </button>
                            <button class="maCK-button" title="Click vào đây để chọn mã chứng khoán cần theo dõi">
                                Chọn mã CK
                                <br>
                                cần theo dõi
                            </button>
                            <button class="maCK-button" title="Click vào đây để chọn mã chứng khoán cần theo dõi">
                                Chọn mã CK
                                <br>
                                cần theo dõi
                            </button>
                            <button class="maCK-button" title="Click vào đây để chọn mã chứng khoán cần theo dõi">
                                Chọn mã CK
                                <br>
                                cần theo dõi
                            </button>
                            <button class="maCK-button" title="Click vào đây để chọn mã chứng khoán cần theo dõi">
                                Chọn mã CK
                                <br>
                                cần theo dõi
                            <button class="btn btn-primary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></button>
                        </div>
                    </div>
            </div>
         </div>
       
    </header>

    <div>
        @Html.Partial("Navbar")
    </div>
    
    <div class="container">
        <main role="main" class="pb-3">
            <div class="row">
                <div class="col-md-7">
                    <h1 style="font-size: 30px;"></h1>
                </div>
            </div>
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - StockInfo.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
